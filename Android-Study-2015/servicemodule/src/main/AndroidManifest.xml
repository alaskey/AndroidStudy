<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sky.activity">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sky.binder.MainActivity" />
        <activity android:name="com.sky.binder.LocalActivity"/>
        <activity android:name="com.sky.binder.ProcessActivity"/>
        <activity android:name="com.sky.handler.MainActivity" />
        <activity android:name="com.sky.service.MainActivity" />

        <service android:name="com.sky.binder.LocalService"/>
        <service android:name="com.sky.binder.ProcessService" android:process=":remote"/>
        <service android:name="com.sky.service.ServiceDemo" />

    </application>

</manifest>
